<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
<app.runtime>4.10.1</app.runtime>
<mule.maven.plugin.version>4.5.3</mule.maven.plugin.version>
</properties>
<build>
<plugins>
<plugin>
<groupId>org.mule.tools.maven</groupId>
<artifactId>mule-maven-plugin</artifactId>
<version>4.5.3</version>
<extensions>true</extensions>
<configuration>
<cloudhub2Deployment>
<uri>https://eu1.anypoint.mulesoft.com</uri>
<provider>MC</provider>
<environment>Sandbox</environment>
<target>CloudHub-EU-Central-1</target>
<muleVersion>4.10.1</muleVersion>
<!--  OBBLIGATORIO CH2  -->
<connectedAppClientId>616ea71301434d59879294e352eba925</connectedAppClientId>
<connectedAppClientSecret>69aAc020D9d143868719dF3221878b19</connectedAppClientSecret>
<connectedAppGrantType>client_credentials</connectedAppGrantType>
<applicationName>pipeline</applicationName>
<businessGroupId>36ee1c61-389a-49a0-9f84-c5d213d44ca7</businessGroupId>
<replicas>1</replicas>
<vCores>0.1</vCores>
</cloudhub2Deployment>
<classifier>mule-application</classifier>
</configuration>
</plugin>
<plugin>
<groupId>org.mule.tools.maven</groupId>
<artifactId>exchange-mule-maven-plugin</artifactId>
<version>0.1.4</version>
<configuration>
<skip>true</skip>
</configuration>
</plugin>
<plugin>
<groupId>org.apache.maven.plugins</groupId>
<artifactId>maven-clean-plugin</artifactId>
<version>3.2.0</version>
</plugin>
