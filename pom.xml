</pluginRepositories>
<distributionManagement>
<repository>
<id>anypoint-exchange-v3</id>
<url>https://maven.eu1.anypoint.mulesoft.com/api/v3/organizations/31f093b5-a64d-4232-8dd8-1a0bcce89388/maven</url>
<layout>default</layout>
</repository>
<snapshotRepository>
